<%= render 'layouts/header' %>
<div class='container'>
<p id="notice"><%= notice %></p>

<h1>Halalfoodhunt SEO</h1>

<table>
  <thead>
    <tr>
      <th colspan="2">Page</th>
      <th colspan="8">Meta description</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @halalfoodhunt_seos.each do |halalfoodhunt_seo| %>
      <tr>
        <td><% if halalfoodhunt_seo.about_us == true %>
        <span class="label label-success">ABOUT US</span>
        <% end %>
        </td>
        <td><%= halalfoodhunt_seo.meta_description %></td>
        <td><%= link_to 'Show', halalfoodhunt_seo %></td>
        <td><%= link_to 'Edit', edit_halalfoodhunt_seo_path(halalfoodhunt_seo) %></td>
        <td><%= link_to 'Destroy', halalfoodhunt_seo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Halalfoodhunt seo', new_halalfoodhunt_seo_path %>
</div>
