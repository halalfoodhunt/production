<% @places_menus.each do |places_menu| %>
 <tr>
  <td class="col-md-9"><%= places_menu.name %></td>
  <td class="col-md-1"><%= link_to edit_places_menu_path(places_menu), remote: true do %>
    <i class="fa fa-pencil" style="color:white"></i><% end %>
  </td>
  <td class="col-md-1"><%= link_to places_menu, method: :delete, data: { confirm: 'Are you sure?' } do %>
    <i class="fa fa-trash-o" style="color:white"></i><% end %>
  </td>
</tr>
<% end %>