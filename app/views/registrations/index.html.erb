<div class="container">
        <h3 class="title">Listing registrations</h3>
        <table class="table table-hover">
          <thead>
            <tr>
             <th>Name</th>
      <th>Ic number</th>
      <th>Date of birth</th>
      <th>Contact number</th>
      <th>Email</th>
      <th>Address 1</th>
      <th>Address 2</th>
      <th>City</th>
      <th>Zipcode</th>
      <th>Notes</th>
              <th colspan="3"></th>
            </tr>
          </thead>
          <tbody>
             <% @registrations.each do |registration| %>
              <tr>
                <td><%= registration.name %></td>
        <td><%= registration.ic_number %></td>
        <td><%= registration.date_of_birth %></td>
        <td><%= registration.contact_number %></td>
        <td><%= registration.email %></td>
        <td><%= registration.address_1 %></td>
        <td><%= registration.address_2 %></td>
        <td><%= registration.city %></td>
        <td><%= registration.zipcode %></td>
        <td><%= registration.notes %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
