<div class="col-md-10 col-md-offset-1">
  <div class="col-xs-12 col-sm-12 col-md-2">
    <h6>SEARCH AN ECOMMER</h6>
  </div>
  <div class="col-xs-12 col-sm-12 col-md-4">
    <div class="form-group" style="margin-bottom: 0; background: rgba(255,255,255,1); color:black;">
      <%= f.text_field :business_name_or_operating_address_cont, class: "form-control" %>
    </div>
  </div>
  <div class="col-xs-12 col-sm-6 col-md-2">
    <%= f.collection_select(:service_types_id_eq_any, ServiceType.all, :id, :name, {:include_blank => "Service Types"}, {:class=>'form-control'}) %>
  </div>
  <div class="col-xs-12 col-sm-6 col-md-2">
    <%= f.collection_select(:food_types_id_eq_any, FoodType.all, :id, :name, {:include_blank => "Food Types"}, {:class=>'form-control'}) %>
  </div>
  <div class="col-xs-12 col-sm-12 col-md-2">
        <%= f.submit "Search", class: "btn btn-raised btn-block btn-danger" %>
  </div>
</div>